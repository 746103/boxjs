#!name= 中国联通@prod 
#!desc= 中国联通@author=gsonhub @version=j3dd

[General]
force-http-engine-hosts = %APPEND%  10010.log 10010.json

[Script]

# 轮询 查询中国联通流量
unicom-for-cron = type=cron,cronexp=*/1 * * * *,timeout=10,script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/unicom.js?v=j3dd

# 查询中国联通流量接口服务  http://10010.json 查询联通日志 http://10010.log 
unicom-http-service = type=http-request,pattern=^https?:\/\/10010\.(json|log),script-path=https://cdn.jsdelivr.net/gh/gsons/boxjs/dist/unicom.js?v=j3dd,requires-body=true,max-size=0,timeout=10

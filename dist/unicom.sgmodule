#!name= 中国联通@prod 
#!desc= 中国联通@author=gsonhub @version=g6bx

[General]
force-http-engine-hosts = %APPEND%  10010.log 10010.json

[Script]

# 轮询 查询中国联通流量
10010-for-cron = type=cron,cronexp=*/1 * * * *,timeout=10,script-path=https://raw.githubusercontent.com/gsons/boxjs/main/dist/unicom.js?v=g6bx

# 查询中国联通流量接口服务  http://10010.json 查询联通日志 http://10010.log 
10010-http-service = type=http-request,pattern=^https?:\/\/10010\.(json|log),script-path=https://raw.githubusercontent.com/gsons/boxjs/main/dist/unicom.js?v=g6bx,requires-body=true,max-size=0,timeout=10

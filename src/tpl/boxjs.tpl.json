            ],
            "scripts": [
                {
                    "name": "中国电信流量查询",
                    "script": "${baseUrl}/10000.js?v=${version}"
                }
            ],
            "keys": [
                "gsonhub.10000.mobile",
                "gsonhub.10000.password",
                "gsonhub.10000.token",
                "gsonhub.10000.mute",
                "gsonhub.10000.vvv_flow",
                "gsonhub.10000.login_num",
                "gsonhub.10000.login_info"
            ],

            "author": "@gsonhub",
            "settings": [
                {
                    "id": "gsonhub.10000.mobile",
                    "name": "手机号",
                    "val": "",
                    "type": "text",
                    "desc": "手机号"
                },
                {
                    "id": "gsonhub.10000.password",
                    "name": "密码",
                    "val": "",
                    "type": "text",
                    "desc": "密码"
                },
                {
                    "id": "gsonhub.10000.mute",
                    "name": "禁音模式",
                    "val": false,
                    "type": "boolean"
                }
            ]
        }
    ]
}